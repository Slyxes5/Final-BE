generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model admin {
  id    Int     @id @default(autoincrement())
  nama  String? @db.VarChar(255)
  no_hp Int?
}

model customer {
  id     Int     @id @default(autoincrement())
  nama   String? @db.VarChar(255)
  alamat String? @db.VarChar(255)
  no_hp  Int?
}
