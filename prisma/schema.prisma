generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model customer {
  id     Int     @id @default(autoincrement())
  nama   String? @db.VarChar(255)
  alamat String? @db.VarChar(255)
  no_hp  Int?
}

model sewa {
  id                  Int      @id @default(autoincrement())
  Tanggal_Pengambilan DateTime
  Tanggal_Pengembalian DateTime
  no_hp               Int
  id_customer         Int
  id_mobil            Int
}

